   "outputs": [],
   "source": [
    "###BEGIN SOLUTION\n",
    "n = 100 # num of docs in corpus \n",
    "df_range = 100\n",
    "IDF = []\n",
    "DF = []\n",
    "for df in range(1, df_range + 1):\n",
    "    idf_score = idf(n, df)\n",
    "    IDF.append(idf_score)\n",
    "    DF.append(df)\n",
    "###END SOLUTION"
   ]
